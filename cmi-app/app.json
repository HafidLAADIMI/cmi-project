{
  "expo": {
    "name": "Afoud-printer",
    "slug": "Afoud-printer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.afoudprinter"
    },
    "android": {
      "permissions": [
        "INTERNET",
        "VIBRATE",
        "CAMERA"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.afoudprinter",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "afoudprinter"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "scheme": "cmipaymentapp",
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "94bd22c8-6486-4fb4-8541-74412df3f4ec"
      }
    }
  }
}
